kick       {usage : n!kick @user or [userid] reason}
ban        {usage : n!ban @user or [userid] reason}
mute       {usage : n!mute @user or [userid] reason}
unmute     {usage : n!unmute @user reason}
warns      {usage : n!warns @user [checking your warns]}
warn       {usage : n!warn @user or [userid] reason}
slowmode   {usage : n!slowmode time [exmp 10]}
unban      {usage : n!unban @userid reason}
nick       {usage : n!nick @user nickname}
addrole    {usage : n!addrole @user reason}
removerole {usage : n!removerole @user reason}
purge      {usage : n!purge [number of messages]}
clearwarn  {usage : n!clearwarn @user reason}
lockdown   {usage : n!lockdown timing [exmp : 1sec/1min/1hour}
say        {usage : n!say message}
tempmute   {usage : n!tempmute @user reason}
dm       {usage : n!dm @user or [userid] message}
